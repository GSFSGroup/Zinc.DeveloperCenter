# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,py,yml,yaml,json,json.raw}]
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false

dotnet_diagnostic.S5766.severity = none