{
  "name": "zn-developercenter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:cicd": "npm run lint && ng build --no-progress",
    "test:cicd": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "restore-deps:cicd": "npm ci",
    "build": "npm run lint && ng build --configuration development",
    "watch": "ng build --configuration development --watch",
    "test": "ng test --watch=false",
    "test:watch": "ng test --watch=true",
    "test:watch:headless": "ng test --watch=true --browsers=ChromeHeadlessCI",
    "cover": "ng test --code-coverage --watch=false",
    "lint": "npm-run-all --parallel lint:ts lint:styles",
    "lint:fix": "npm-run-all --parallel lint:ts:fix lint:styles:fix",
    "lint:ts": "ng lint",
    "lint:ts:fix": "ng lint --fix",
    "lint:styles": "stylelint \"projects/**/*.{css,scss}\" ",
    "lint:styles:fix": "stylelint --fix \"projects/**/*.{css,scss}\" ",
    "postinstall": "ngcc --properties main -l warn"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.0",
    "@angular-eslint/builder": "^13.1.0",
    "@angular-eslint/eslint-plugin": "^13.1.0",
    "@angular-eslint/eslint-plugin-template": "^13.1.0",
    "@angular-eslint/schematics": "^13.1.0",
    "@angular-eslint/template-parser": "^13.1.0",
    "@angular/animations": "^13.3.0",
    "@angular/cdk": "^13.3.1",
    "@angular/cli": "^13.3.0",
    "@angular/common": "^13.3.0",
    "@angular/compiler": "^13.3.0",
    "@angular/compiler-cli": "^13.3.0",
    "@angular/core": "^13.3.0",
    "@angular/elements": "^13.3.0",
    "@angular/forms": "^13.3.0",
    "@angular/language-service": "^13.3.0",
    "@angular/localize": "^13.3.0",
    "@angular/material": "^13.3.1",
    "@angular/platform-browser": "^13.3.0",
    "@angular/platform-browser-dynamic": "^13.3.0",
    "@angular/router": "^13.3.0",
    "@gsfsgroup/kr-shell-services": "^0.7.2",
    "@gsfsgroup/kr-ui-components": "^1.0.1",
    "@types/jasmine": "^4.0.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "codelyzer": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jsdoc": "^38.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.29.4",
    "jasmine-core": "^4.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.0",
    "ngx-build-plus": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^4.0.3",
    "protractor": "^7.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "zone.js": "^0.11.5"
  },
  "dependencies": {
    "ngx-markdown": "^13.1.0"
  }
}
